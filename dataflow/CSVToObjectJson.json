{
	"name": "CSVToObjectJson",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_abs_csv",
						"type": "DatasetReference"
					},
					"name": "sourceJson"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_json_output",
						"type": "DatasetReference"
					},
					"name": "sinkJson"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {faixa_etaria;sexo;raca_cor;municipio;grupo;categoria;vacina_fabricante;descricao_dose;cnes;sistema_origem;data_vacinacao} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceJson",
				"sourceJson sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['jsonVacinados.json'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinkJson"
			]
		}
	}
}