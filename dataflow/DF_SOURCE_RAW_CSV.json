{
	"name": "DF_SOURCE_RAW_CSV",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_generic",
						"type": "DatasetReference"
					},
					"name": "sourceApi"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregateDose"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cpf as string,",
				"          sexo as string,",
				"          grupo as string,",
				"          vacina as string,",
				"          dose as short,",
				"          data_vacinacao as date,",
				"          local_vacinacao as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> sourceApi",
				"sourceApi aggregate(groupBy(cpf),",
				"     dose = count(cpf),",
				"          sexo = first(sexo),",
				"          grupo = first(grupo),",
				"          first_data_vacinacao = first(data_vacinacao),",
				"          last_data_vacinacao2 = last(data_vacinacao),",
				"          first_local_vacinacao = first(local_vacinacao),",
				"          last_local_vacinacao = last(local_vacinacao)) ~> aggregateDose"
			]
		}
	}
}