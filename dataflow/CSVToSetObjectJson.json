{
	"name": "CSVToSetObjectJson",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_blob_storage",
						"type": "DatasetReference"
					},
					"name": "sourceCsv"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_json_blob_storage",
						"type": "DatasetReference"
					},
					"name": "rawJson"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          faixa_etaria as string,",
				"          idade as short,",
				"          sexo as string,",
				"          raca_cor as string,",
				"          municipio as string,",
				"          grupo as string,",
				"          categoria as string,",
				"          lote as string,",
				"          vacina_fabricante as string,",
				"          descricao_dose as short,",
				"          cnes as string,",
				"          sistema_origem as string,",
				"          data_vacinacao as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> sourceCsv",
				"sourceCsv sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['jsonSetVacinados.json'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> rawJson"
			]
		}
	}
}