{
	"name": "DF_SOURCE",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_generic",
						"type": "DatasetReference"
					},
					"name": "sourceApi"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregateCpf"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cpf as string,",
				"          sexo as string,",
				"          grupo as string,",
				"          dose as short,",
				"          data_vacinacao as date,",
				"          local_vacinacao as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> sourceApi",
				"sourceApi aggregate(groupBy(cpf),",
				"     dose = count(cpf),",
				"          sexo = first(sexo),",
				"          grupo = first(grupo),",
				"          first_data_vacinacao = first(data_vacinacao),",
				"          last_data_vacinacao = last(data_vacinacao),",
				"          first_local_vacinacao = first(local_vacinacao),",
				"          last_local_vacinacao = last(local_vacinacao)) ~> aggregateCpf"
			]
		}
	}
}