{
	"name": "DF_SOURCE",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_generic",
						"type": "DatasetReference"
					},
					"name": "sourceApi"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filterCpfNull"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceApi",
				"sourceApi filter(cpf!='null') ~> filterCpfNull"
			]
		}
	}
}