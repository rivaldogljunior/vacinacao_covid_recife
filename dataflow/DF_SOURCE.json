{
	"name": "DF_SOURCE",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_generic",
						"type": "DatasetReference"
					},
					"name": "sourceApi"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filterCpfNull"
				},
				{
					"name": "aggregateDistinctRows"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cpf as string,",
				"          nome as string,",
				"          sexo as string,",
				"          grupo as string,",
				"          vacina as string,",
				"          lote as string,",
				"          dose as short,",
				"          data_vacinacao as date,",
				"          local_vacinacao as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> sourceApi",
				"sourceApi filter(cpf!='null') ~> filterCpfNull",
				"filterCpfNull aggregate(groupBy(cpf,",
				"          nome),",
				"     sexo = last(sexo),",
				"          grupo = last(grupo),",
				"          doses = count(dose)) ~> aggregateDistinctRows"
			]
		}
	}
}