{
	"name": "raw_to_trusted",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_generic",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_parquet_generic",
						"type": "DatasetReference"
					},
					"name": "ParquetTrusted"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          faixa_etaria as string,",
				"          sexo as short,",
				"          raca_cor as string,",
				"          municipio as string,",
				"          grupo as string,",
				"          categoria as string,",
				"          lote as string,",
				"          vacina_fabricante as string,",
				"          ordem_dose as string,",
				"          cnes as short,",
				"          sistema_origem as string,",
				"          data_vacinacao as string,",
				"          {} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false,",
				"     wildcardPaths:['*.csv*']) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     partitionFileNames:['parquetVacinados.parquet'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> ParquetTrusted"
			]
		}
	}
}